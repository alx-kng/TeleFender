<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.Circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.LessRounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.Square" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.LeftRounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeBottomLeft">10dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.Button.RightRounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomRight">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.CardView.Square" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Tele.CardView.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="Widget.Tele.Toolbar.Primary" parent="@style/Widget.MaterialComponents.Toolbar.Primary">
        <item name="colorControlNormal">@color/icon_white</item>
    </style>

    <style name="ThemeOverlay.Tele.ActionBar" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="colorControlNormal">@color/icon_white</item>
    </style>

</resources>
